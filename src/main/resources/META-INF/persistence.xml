<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="admglp">
        <class>mx.unam.dgtic.admglp.vo.Acceso</class>
        <class>mx.unam.dgtic.admglp.vo.Articulo</class>
        <class>mx.unam.dgtic.admglp.vo.Asentamiento</class>
        <class>mx.unam.dgtic.admglp.vo.Cliente</class>
        <class>mx.unam.dgtic.admglp.vo.Comentario</class>
        <class>mx.unam.dgtic.admglp.vo.Contra</class>
        <class>mx.unam.dgtic.admglp.vo.Direccion</class>
        <class>mx.unam.dgtic.admglp.vo.Empleado</class>
        <class>mx.unam.dgtic.admglp.vo.Empleadopedido</class>
        <class>mx.unam.dgtic.admglp.vo.Empleadoturno</class>
        <class>mx.unam.dgtic.admglp.vo.Estado</class>
        <class>mx.unam.dgtic.admglp.vo.Global</class>
        <class>mx.unam.dgtic.admglp.vo.Municipio</class>
        <class>mx.unam.dgtic.admglp.vo.Orden</class>
        <class>mx.unam.dgtic.admglp.vo.Pedido</class>
        <class>mx.unam.dgtic.admglp.vo.Rol</class>
        <class>mx.unam.dgtic.admglp.vo.Transporte</class>
        <class>mx.unam.dgtic.admglp.vo.Turno</class>
        <class>mx.unam.dgtic.admglp.vo.Usuario</class>
        <properties>
            <property name="jakarta.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver" />
            <property name="jakarta.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/admglp" />
            <property name="jakarta.persistence.jdbc.user" value="root" />
            <property name="jakarta.persistence.jdbc.password" value="java" />
        </properties>
    </persistence-unit>

